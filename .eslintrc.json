{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["airbnb-base", "plugin:codeceptjs/recommended"],
  "plugins": ["codeceptjs"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": "off",
    "no-restricted-globals": "off",
    "import/no-mutable-exports": "off"
  }
}
